{
  "cli": {
    "version": ">= 0.59.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "main": {
      "channel": "main",
      "distribution": "internal",
      "node": "14.20.0",
      "env": {
        "APP_VARIANT": "main"
      }
    },
    "production": {
      "channel": "production",
      "node": "14.20.0",
      "env": {
        "APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
